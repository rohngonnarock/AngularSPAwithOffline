<!DOCTYPE html>
<html manifest="~/AppCache/AppManifest" type="text/cache-manifest" ng-app="AwesomeAngularMVCApp" ng-controller="LandingPageController">
<head>
    <title ng-bind="models.helloAngular"></title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
</head>
<body>
    @RenderBody()
    <footer>
        <h1>Footer</h1>
    </footer>
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-ui-router.min.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>
    @Scripts.Render("~/bundles/AwesomeAngularMVCApp")
    @RenderSection("Scripts", required: false)
    <script>
    $(document).ready(function () {
        alert('Page Load Called');
    });
    </script>
</body>
</html>