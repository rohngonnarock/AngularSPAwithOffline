<!DOCTYPE html>
<html manifest="~/AppCache/AppManifest" type="text/cache-manifest" ng-app="AwesomeAngularMVCApp" ng-controller="LandingPageController">
<head>
    <title ng-bind="models.helloAngular"></title>
      
      @Styles.RenderFormat("<link href=\"{0}\" type=\"text/css\" rel="stylesheet"/>","~/Content/css")
    </head>
<body>
    @RenderBody()
    <footer>
        <h1>Footer </h1>
    </footer>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AwesomeAngularMVCApp")
    @RenderSection("Scripts", required: false)
    <script>
    $(document).ready(function () {
        alert('Page Load Complete');
    });
    </script>
</body>
</html>
