<!DOCTYPE html>
<html manifest="~/AppCache/AppManifest" type="text/cache-manifest" ng-app="AwesomeAngularMVCApp" ng-controller="LandingPageController">
<head>
    <title ng-bind="models.helloAngular"></title>
    </head>
<body>
    @RenderBody()
    <footer>
        <h1>Footer </h1>
    </footer>
    @Styles.RenderFormat("<link href=\"{0}\" rel='stylesheet' async />", "~/Content/css")
    @Scripts.RenderFormat("<script type=\"text/javascript\" src=\"{0}\" async></script>", "~/bundles/modernizr")
    @Scripts.RenderFormat("<script type=\"text/javascript\" src=\"{0}\"></script>", "~/bundles/bootstrap")
    @Scripts.RenderFormat("<script type=\"text/javascript\" src=\"{0}\"></script>", "~/bundles/AwesomeAngularMVCApp")
    @RenderSection("Scripts", required: false)
</body>
</html>
